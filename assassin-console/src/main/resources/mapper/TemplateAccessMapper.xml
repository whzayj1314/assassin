<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.assassinx.assassin.console.mapper.TemplateAccessMapper">

    <select id="insertTemplateAccess">
        INSERT INTO template_access VALUES
        <foreach collection="accessIds" item="accessId" separator=",">
            (#{templateId},#{accessId})
        </foreach>
    </select>

</mapper>
